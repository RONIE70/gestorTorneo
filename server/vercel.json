{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { 
      
      "src": "/fichar", 
      "dest": "index.js" 
    },
    {
      "src": "/AdminDelegado",
      "dest": "/index.html"
    },
      
    { "src": "/(.*)", 
      "dest": "index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PATCH, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ]
}